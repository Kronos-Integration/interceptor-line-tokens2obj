{
  "name": "kronos-interceptor-line-tokens2obj",
  "version": "0.0.0-semantic-release",
  "main": "dist/module.js",
  "module": "src/module.js",
  "description": "Splits a line by a sparator into tokens.",
  "keywords": [
    "kronos-interceptor",
    "stream",
    "line",
    "parser"
  ],
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "nyc --temp-directory build/nyc npm test",
    "doc": "./node_modules/.bin/jsdoc lib/*.js",
    "test": "mocha tests/*-test.js",
    "semantic-release": "semantic-release",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js >README.md",
    "pretest": "rollup -c",
    "posttest": "markdown-doctest",
    "precover": "rollup -c",
    "prepare": "rollup -c"
  },
  "dependencies": {
    "kronos-interceptor": "^2.6.0",
    "z-schema": "3.22.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "kronos-test-interceptor": "3.1.1",
    "mocha": "^5.2.0",
    "semantic-release": "^15.9.8",
    "jsdoc-to-markdown": "^4.0.1",
    "markdown-doctest": "^0.9.1",
    "rollup": "^0.64.1",
    "nyc": "^13.0.1"
  },
  "engines": {
    "node": ">=8.11.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-interceptor-line-tokens2obj.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-interceptor-line-tokens2obj/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-interceptor-line-tokens2obj#readme",
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./coverage"
  },
  "xo": {
    "space": true
